{
  "name": "box-node-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate app that gets you up and running with Box Platform and authentication",
  "scripts": {
    "watch": "watchify styles/css/style.js -t scssify styles/js/*.js -o public/bundle.js -dv",
    "build": "browserify styles/css/style.js -t scssify styles/js/*.js > public/bundle.js",
    "start": "npm run build && nodemon app.js",
    "start-dev": "npm run watch & npm start"
  },
  "author": "Matt Mitchell",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "box-node-sdk": "^1.7.0",
    "browserify": "^14.4.0",
    "config": "^1.26.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.3.5",
    "ejs": "^2.3.3",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "mongoose": "^4.10.8",
    "nodemon": "^1.11.0",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "path": "^0.12.7",
    "scssify": "^2.3.0"
  },
  "devDependencies": {
    "watchify": "^3.9.0"
  }
}
